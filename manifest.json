{
  "name": "UI Opt-In Switches",
  "description": "Chrome App for easily accessing Opt-In Experiences and saving for future use",
  "version": "1.0.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "switch-16x16.png", "128": "switch-256x256.png" },
  "permissions": [
        "https://fonts.googleapis.com/",
        "notifications",
        "storage"
    ]
}
